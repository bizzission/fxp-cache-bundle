{
    "name": "fxp/cache-bundle",
    "type": "symfony-bundle",
    "description": "Add extra features to Symfony PSR-6 Cache Component",
    "keywords": ["cache", "psr-6", "namespace", "prefix"],
    "homepage": "https://github.com/fxpio/fxp-cache-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^4.2.0",
        "fxp/cache": "^1.1.0"
    },
    "autoload": {
        "psr-4": { "Fxp\\Bundle\\CacheBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
